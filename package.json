{
  "name": "ide-latex",
  "main": "./dist/main",
  "version": "0.0.0",
  "description": "An all-in-one LaTeX for Atom package",
  "keywords": [
    "latex",
    "ide"
  ],
  "repository": "https://github.com/Aerijo/ide-latex",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "tree-sitter-biber": "0.4.0"
  },
  "devDependencies": {
    "@types/atom": "latest",
    "atom-languageclient": "^0.9.9",
    "tslint": "latest",
    "typescript": "latest"
  },
  "scripts": {
    "build": "tsc"
  },
  "configSchema": {
    "serverPath": {
      "description": "An absolute path to the the server executable",
      "type": "string",
      "default": ""
    }
  },
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    }
  }
}
